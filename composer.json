{
    "name": "shimoning/bom",
    "version": "0.2.0",
    "description": "Unicodeの種類に合わせてBOMを取得するライブラリ",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shimon Haga"
        }
    ],
    "keywords": [
        "bom",
        "unicode",
        "utf8",
        "utf-8",
        "utf16",
        "utf-16"
    ],
    "autoload": {
        "psr-4": {
            "Shimoning\\Bom\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^7 | ^8 | ^9"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testsuite Latest --testdox",
        "test:bc": "vendor/bin/phpunit --testsuite BackwardCompatible --testdox",
        "test:all": "vendor/bin/phpunit --testdox"
    }
}
